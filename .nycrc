{
	"extends": "@istanbuljs/nyc-config-typescript",
	"require": ["ts-node/register"],
	"extension": [".ts"],
	"reporter": ["html", "text"],
	"sourceMap": true,
	"instrument": true,
	"temp-dir": ".nyc_output",
	"report-dir": "coverage",
	"all": true,
	"include": ["src"],
	"exclude": ["tests"],
	"check-coverage": false,
	"branches": 80,
	"lines": 80,
	"functions": 80,
	"statements": 80
}